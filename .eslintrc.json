{
    "overrides": [
      {
        "files": ["**/*.js"],
        "env": {
          "node": true
        },
        "extends": ["airbnb-base"],
        "parserOptions": {
          "sourceType": "commonjs"
        }
      },
      {
        "files": ["**/*.{js,mjs,cjs}"],
        "env": {
          "browser": true
        },
        "globals": {
          "browser": "readonly"
        }
      }
    ]
  }
  